{
  "name": "json-schema-builder-react",
  "version": "1.0.1",
  "description": "A React component library for building and editing JSON schemas visually",
  "type": "module",
  "license": "MIT",
  "author": "Martin Arusalu",
  "private": false,
  "repository": {
    "type": "git",
    "url": "https://github.com/martin-arusalu/json-schema-editor.git"
  },
  "homepage": "https://martin-arusalu.github.io/json-schema-editor",
  "keywords": [
    "json-schema",
    "schema-builder",
    "react",
    "typescript",
    "visual-builder",
    "json-schema-builder"
  ],
  "main": "./dist-lib/index.cjs",
  "module": "./dist-lib/index.js",
  "types": "./dist-lib/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist-lib/index.d.ts",
      "import": "./dist-lib/index.js",
      "require": "./dist-lib/index.cjs"
    },
    "./styles.css": "./dist-lib/style.css"
  },
  "files": [
    "dist-lib",
    "README.md",
    "LICENSE"
  ],
  "sideEffects": [
    "*.css"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "build:lib": "BUILD_MODE=library vite build && npm run build:types",
    "build:types": "tsc --project tsconfig.lib.json",
    "lint": "eslint .",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json,css}\"",
    "preview": "vite preview",
    "check": "tsc",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d dist",
    "prepublishOnly": "npm run build:lib"
  },
  "peerDependencies": {
    "@radix-ui/react-checkbox": "^1.0.0",
    "@radix-ui/react-dialog": "^1.0.0",
    "@radix-ui/react-label": "^2.0.0",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-slot": "^1.0.0",
    "@radix-ui/react-tooltip": "^1.0.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.0.0",
    "lucide-react": "^0.507.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0",
    "tailwind-merge": "^3.4.0"
  },
  "devDependencies": {
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@tailwindcss/typography": "^0.5.15",
    "@tailwindcss/vite": "^4.1.3",
    "@types/json-schema": "^7.0.15",
    "@types/node": "^22.12.0",
    "@types/react": "^18.3.11",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/eslint-plugin": "^8.46.3",
    "@typescript-eslint/parser": "^8.46.3",
    "@vitejs/plugin-react": "^4.7.0",
    "autoprefixer": "^10.4.20",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "eslint": "^9.39.1",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^7.0.1",
    "gh-pages": "^6.3.0",
    "lucide-react": "^0.453.0",
    "postcss": "^8.4.47",
    "prettier": "^3.6.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "tailwind-merge": "^2.6.0",
    "tailwindcss": "^3.4.17",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.5",
    "typescript": "5.6.3",
    "vite": "^7.2.4",
    "wouter": "^3.3.5"
  }
}
